HANDLE :: #type u64;
DWORD  :: #type u32;
BOOL   :: #type i32;

GetStdHandle :: (i: i32)-> u64 {
  return #dynamic_import((i32) -> u64, "kernel32.dll", "GetStdHandle")(i);
}
WriteConsoleA :: (handle: HANDLE, string: *void, count: DWORD, out_count: *DWORD) -> BOOL {
  return #dynamic_import((HANDLE, *void, DWORD, *DWORD, *void) -> u32,
                         "kernel32.dll", "WriteConsoleA") (handle, string, count, out_count, nullptr);
}