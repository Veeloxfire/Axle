hash :: (slice: []u8) -> u64 {
  hash_v : u64 = 0xcbf29ce484222325;
  i: u64 = 0;

  while(i < slice.len) {
    hash_v = hash_v * 0x100000001b3;
    hash_v = hash_v ^ cast(u64, slice[i]);

    i = i + 1;
  }

  return hash_v;
}

main :: () -> u64 {
  arr :=  "hello";

  return hash(cast([]u8, arr[,]));
}
