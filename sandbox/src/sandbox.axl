HANDLE :: #type u64;
DWORD :: #type u32;

GetStdHandle :: #dynamic_link((i32) -> u64, "kernel32.dll", "GetStdHandle");
WriteConsoleA :: #dynamic_link((HANDLE, *void, DWORD, *DWORD, *void) -> u32, "kernel32.dll", "WriteConsoleA");

main :: () -> u64 {
  h := GetStdHandle(-11i32);
  message := 0x0000006f6c6c6568;

  return WriteConsoleA(h, cast(*void, &message), 5u32, cast(*u32, nullptr), nullptr);
}